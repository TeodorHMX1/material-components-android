<resources>

  <!-- Style to use for OutlinedLayout in the theme. -->
  <attr name="outlinedLayoutStyle" format="reference" />

  <declare-styleable name="OutlinedLayout">
    <!-- The text to display as prefix text in the text input area. -->
<!--    <attr name="title" format="string" />-->
<!--    &lt;!&ndash; The text to display as prefix text in the text input area. &ndash;&gt;-->
<!--    <attr name="titleEnabled" format="boolean" />-->
    <!-- TextAppearance of the hint in the collapsed floating label. -->
    <attr name="titleAppearance" format="reference" />
    <!-- Text color of the hint in the collapsed floating label.
         If set, this takes precedence over hintTextAppearance. -->
    <attr name="titleColor" format="color" />
  </declare-styleable>

</resources>
